{
  "name": "dustbin-react",
  "version": "1.0.2",
  "description": "React Hooks library for remote data fetching",
  "main": "./build/index.js",
  "module": "./build/index.esm.js",
  "umd": "./build/index.umd.js",
  "types": "./build/index.d.ts",
  "typings": "./build/index.d.ts",
  "scripts": {
    "clear": "rm -rf build/",
    "build": "clear && rollup --environment ENV:production -c --bundleConfigAsCjs",
    "build:dev": "clear && rollup --environment ENV:development -c --bundleConfigAsCjs",
    "dev": "clear && rollup --environment ENV:development -w -c --bundleConfigAsCjs "
  },
  "keywords": [
    "react",
    "state libs",
    "hooks",
    "request",
    "cache",
    "fetch",
    "restful request libs"
  ],
  "author": "cavey1230",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "files": [
    "build"
  ]
}
