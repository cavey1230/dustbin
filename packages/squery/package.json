{
  "name": "squery",
  "version": "0.0.1",
  "description": "",
  "main": "./build/index.js",
  "module": "./build/index.esm.js",
  "umd": "./build/index.umd.js",
  "types": "./build/index.d.ts",
  "typings": "./build/index.d.ts",
  "scripts": {
    "clear": "rm -rf build/",
    "build:types": "npm run clear && tsc -b tsconfig.types.json",
    "build": "npm run build:types && rollup --environment ENV:production -c --bundleConfigAsCjs",
    "build:dev": "npm run build:types && rollup --environment ENV:development -c --bundleConfigAsCjs",
    "dev": "npm run build:types && rollup --environment ENV:development -w -c --bundleConfigAsCjs "
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "keywords": [
    "react",
    "query",
    "storage"
  ],
  "author": "cavey1230",
  "license": "MIT",
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.3.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^3.9.1",
    "rollup-plugin-dts": "^5.1.1",
    "rollup-plugin-typescript2": "^0.34.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  },
  "files": [
    "build"
  ]
}
